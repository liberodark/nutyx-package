# Depends on: 

description="An Web builder"
packager="liberodark"
url="http://www.quickandeasywebbuilder.com/"

name=quick-n-easy-web-builder-6
version=6.0.2

source=(https://www.quickandeasywebbuilder.com/quick-n-easy-web-builder-6_${version}_amd64.deb)

build() {

ar x quick-n-easy-web-builder-6_${version}_amd64.deb
tar xvf data.tar.xz
cp -r opt $PKG
cp -r usr $PKG
}