 # Depends on: cairo gcc-libs gdk-pixbuf2 glib2 glibc gtk2 gtkglext libglvnd libx11 libxcb libxdamage libxext libxfixes libxi libxrandr libxtst pango python-shiboken2 libpulse

description="All-In-One Software for Remote Support"
packager="liberodark"
url="https://anydesk.fr/"

name=anydesk
version=4.0.0

source=(https://download.anydesk.com/linux/anydesk_${version}-1_amd64.deb)

build() {

ar x anydesk_${version}-1_amd64.deb
tar xvf data.tar.xz
cp -r usr $PKG
}